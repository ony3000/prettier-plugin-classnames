{
  "name": "prettier-plugin-classnames",
  "version": "0.1.2",
  "description": "A Prettier plugin that wraps verbose class name based on the `printWidth` option.",
  "repository": "https://github.com/ony3000/prettier-plugin-classnames",
  "keywords": [
    "prettier",
    "plugin",
    "classnames"
  ],
  "author": "Hyeonjong <nianelo4@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/prettier-plugin-classnames.umd.cjs",
  "module": "./dist/prettier-plugin-classnames.js",
  "exports": {
    ".": {
      "import": "./dist/prettier-plugin-classnames.js",
      "require": "./dist/prettier-plugin-classnames.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite build --watch --minify=false",
    "build": "vite build",
    "lint": "eslint src",
    "test": "jest --ci --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/jest": "29.5.2",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-node": "0.3.7",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.5.0",
    "prettier": "~2.8.4",
    "ts-jest": "29.1.0",
    "typescript": "4.9.5",
    "vite": "4.3.9",
    "vite-plugin-dts": "3.0.0-beta.2"
  },
  "peerDependencies": {
    "prettier": "~2.8.4"
  },
  "optionalDependencies": {
    "classnames": "2.3.2",
    "prettier-plugin-merge": "~0.2.0"
  }
}
